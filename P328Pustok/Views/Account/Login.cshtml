@model MemberRegisterViewModel
@{
    ViewData["Title"] = "Login";
}


		<section class="breadcrumb-section">
			<h2 class="sr-only">Site Breadcrumb</h2>
			<div class="container">
				<div class="breadcrumb-contents">
					<nav aria-label="breadcrumb">
						<ol class="breadcrumb">
							<li class="breadcrumb-item"><a href="index.html">Home</a></li>
							<li class="breadcrumb-item active">Login</li>
						</ol>
					</nav>
				</div>
			</div>
		</section>
		<!--=============================================
		=            Login Register page content         =
		=============================================-->
		<main class="page-section inner-page-sec-padding-bottom">
			<div class="container">
				<div class="row">
					<div class="col-sm-12 col-md-12 col-lg-6 col-xs-12 m-auto">
						<form method="post">
							<div class="login-form">
								<h4 class="login-title">Returning Customer</h4>
								<p><span class="font-weight-bold">I am a returning customer</span></p>
								<div class="row">
							    <div class="col-md-12 col-12 mb--15">
								    <label asp-for="UserName">UserName.</label>
								    <input class="mb-0 form-control" asp-for="UserName"
											   placeholder="Enter you username">
								<span class="text-danger" asp-validation-for="UserName"></span>

									</div>
									<div class="col-12 mb--20">
								<label asp-for="Password">Password</label>
								<input class="mb-0 form-control" asp-for="Password" placeholder="Enter your password">
								<span class="text-danger" asp-validation-for="Password"></span>

									</div>
									<div class="text-danger" asp-validation-summary="ModelOnly"></div>
									<div class="col-md-12">
										<button type="sumbit" class="btn btn-outlined">Login</button>
									</div>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
		</main>
	
	